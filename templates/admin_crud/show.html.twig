{% extends 'base.html.twig' %}

{% block title %}User
{% endblock %}

{% block body %}
	<h1>User</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ user.id }}</td>
			</tr>
			<tr>
				<th>Email</th>
				<td>{{ user.email }}</td>
			</tr>
			<tr>
				<th>Roles</th>
				<td>{{ user.roles ? user.roles|json_encode : '' }}</td>
			</tr>
			<tr>
				<th>Password</th>
				<td>{{ user.password }}</td>
			</tr>
			<tr>
				<th>Prenom</th>
				<td>{{ user.prenom }}</td>
			</tr>
			<tr>
				<th>Nom</th>
				<td>{{ user.nom }}</td>
			</tr>
			<tr>
				<th>Telephone</th>
				<td>{{ user.telephone }}</td>
			</tr>
			<tr>
				<th>Pays</th>
				<td>{{ user.pays }}</td>
			</tr>
			<tr>
				<th>Adresse</th>
				<td>{{ user.adresse }}</td>
			</tr>
			<tr>
				<th>ComplementAdresse</th>
				<td>{{ user.complementAdresse }}</td>
			</tr>
			<tr>
				<th>Region</th>
				<td>{{ user.region }}</td>
			</tr>
			<tr>
				<th>Ville</th>
				<td>{{ user.ville }}</td>
			</tr>
			<tr>
				<th>CodePostal</th>
				<td>{{ user.codePostal }}</td>
			</tr>
			<tr>
				<th>Newsletter</th>
				<td>{{ user.newsletter ? 'Yes' : 'No' }}</td>
			</tr>
			<tr>
				<th>NomSociete</th>
				<td>{{ user.nomSociete }}</td>
			</tr>
			<tr>
				<th>Siret</th>
				<td>{{ user.siret }}</td>
			</tr>
			<tr>
				<th>Civilite</th>
				<td>{{ user.civilite ? 'Yes' : 'No' }}</td>
			</tr>
		</tbody>
	</table>

