<div class="col-12 col-sm-6 col-md-4 col-lg-3 mb-5">
	{% if website %}
		<a href="{{ website }}" target="_blank" style="text-decoration: none">
			<div class="card card-services h-100">
				<div class="content-partners">
					<img src="{{ logo }}" alt="">
					{# <h1>{{marque}}</h1> #}
					<p>{{contenu}}</p>
				</div>
			</div>
		</a>
	{% else %}
		<div class="card card-services h-100">
			<div class="content-partners">
				<img src="{{ logo }}" alt="">
				{# <h1>{{marque}}</h1> #}
				<p>{{contenu}}</p>
			</div>
		</div>
	{% endif %}
</div>
